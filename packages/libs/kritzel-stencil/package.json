{
  "name": "kritzel-stencil",
  "version": "0.0.81",
  "description": "Stencil Component Starter",
  "main": "dist/index.cjs.js",
  "module": "dist/index.js",
  "types": "dist/types/index.d.ts",
  "collection": "dist/collection/collection-manifest.json",
  "collection:main": "dist/collection/index.js",
  "unpkg": "dist/stencil/stencil.esm.js",
  "exports": {
    ".": {
      "import": "./dist/stencil/stencil.esm.js",
      "require": "./dist/stencil/stencil.cjs.js"
    },
    "./my-component": {
      "import": "./dist/components/my-component.js",
      "types": "./dist/components/my-component.d.ts"
    },
    "./loader": {
      "import": "./loader/index.js",
      "require": "./loader/index.cjs",
      "types": "./loader/index.d.ts"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ionic-team/stencil-component-starter.git"
  },
  "files": [
    "dist/",
    "loader/"
  ],
  "scripts": {
    "build": "stencil build",
    "start": "stencil build --dev --watch --serve",
    "test": "stencil test --spec --e2e",
    "test.watch": "stencil test --spec --e2e --watchAll",
    "generate": "stencil generate",
    "clean": "rimraf dist .stencil loader node_modules",
    "bump": "npm version patch",
    "release": "npm publish"
  },
  "devDependencies": {
    "@stencil/angular-output-target": "^0.10.2",
    "@stencil/core": "^4.31.0",
    "@stencil/react-output-target": "^1.0.2",
    "@stencil/vue-output-target": "0.10.7",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.14.1",
    "jest": "^29.7.0",
    "jest-cli": "^29.7.0",
    "puppeteer": "^24.6.1"
  },
  "license": "MIT",
  "dependencies": {
    "@types/lodash-es": "^4.17.12",
    "browser-image-compression": "^2.0.2",
    "lodash-es": "^4.17.21",
    "perfect-freehand": "^1.2.2"
  }
}
